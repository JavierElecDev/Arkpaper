<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Home"
    android:background="@drawable/back_apppen">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="274dp"
        android:layout_height="65dp"
        android:layout_marginTop="72dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/arckloby" />

    <ImageView
        android:id="@+id/IMGAdd"
        android:layout_width="94dp"
        android:layout_height="93dp"
        android:layout_marginStart="60dp"
        android:layout_marginTop="224dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/add" />

    <ImageView
        android:id="@+id/IMGsearch"
        android:layout_width="94dp"
        android:layout_height="93dp"
        android:layout_marginTop="92dp"
        android:layout_marginEnd="60dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/IMGEdit"
        app:srcCompat="@drawable/search" />

    <ImageView
        android:id="@+id/IMGDel"
        android:layout_width="94dp"
        android:layout_height="93dp"
        android:layout_marginStart="60dp"
        android:layout_marginTop="92dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/IMGAdd"
        app:srcCompat="@drawable/del" />

    <ImageView
        android:id="@+id/IMGEdit"
        android:layout_width="94dp"
        android:layout_height="93dp"
        android:layout_marginTop="224dp"
        android:layout_marginEnd="60dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/edit" />


</androidx.constraintlayout.widget.ConstraintLayout>